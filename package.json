{
  "name": "astro",
  "homepage": "https://avahak.github.io/astro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "buildtimestamp": "node -e \"const fs = require('fs'); const path = require('path'); const timestamp = new Date().toLocaleString('en-FI', { timeZone: 'Europe/Helsinki', year: 'numeric', month: '2-digit', day: '2-digit', hour: '2-digit', minute: '2-digit' }); const content = `export const BUILD_TIMESTAMP = '${timestamp}';\\n`; fs.writeFileSync(path.join('src', 'buildInfo.ts'), content, 'utf8');\"",
    "dev": "vite",
    "devhost": "vite --host",
    "build": "npm run buildtimestamp && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "d3": "^7.9.0",
    "mathjs": "^14.4.0",
    "pako": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-mathjax": "^2.0.5",
    "react-router-dom": "^7.5.2",
    "three": "^0.176.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.15.2",
    "@types/pako": "^2.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.176.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "typedoc": "^0.28.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3"
  }
}
